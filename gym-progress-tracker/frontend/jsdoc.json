{
    "source": {
      "include": ["./src"],
      "excludePattern": "(node_modules|build)"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
    },
    "opts": {
      "recurse": true,
      "destination": "./docs",
      "readme": "./README.md"
    }
  }